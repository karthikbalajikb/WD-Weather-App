{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins": ["import", "jsx-a11y", "react"],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "no-undef": "off",
    "linebreak-style": 0,
    "react/no-did-mount-set-state": [0],
    "react/forbid-prop-types": [
      1,
      {
        "forbid": ["any"],
        "checkContextTypes": false,
        "checkChildContextTypes": false
      }
    ]
  }
}
